*{
    margin:0;
    padding:0;
}
body{
    padding-top:50px;
    padding-bottom: 50px;
}

.carousel{
    height:400px;
    background-color:#000;
    margin-bottom:25px;
}
.carousel .item{
    height:400px;
    background-color:#000;
}
.carousel img{
    width:100%;
}
.carousel-caption p{
    line-height:1.5em;
    margin-bottom:10px;
    font-size:20px
}
.face_img img{
    weight:100px;
    height:100px;
    border-radius: 50px
}

.summary_container{
    text-align:center;
}

hr .divider{
    margin:30px 0;
}

.container_center > .row{
    text-align:center;
    margin:30px 0;
}
.tabl-list{
    margin-bottom: 20px;
}

.glyphicon {
    float:right;
}

#menu{
    padding-left:0px;
    padding-top: 24px;
    line-height: 30px;
}
.sub_menu{
    text-indent:2em;
}


#main .panel{
    margin-top:12px;
}
.page-header p{
    padding: 6px 17px;
}